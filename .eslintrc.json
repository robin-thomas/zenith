{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": "error",
    "semi": "error",
    "quotes": ["error", "single", { "avoidEscape": true }],
    "no-var": "error",
    "no-console": "error",
    "max-len": [
      "error",
      {
        "code": 120,
        "tabWidth": 2,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ]
  }
}
